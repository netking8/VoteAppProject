<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.votingapp">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/application_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.VotingApp">
        <activity android:name=".exit"></activity>
        <activity android:name=".VoteActivity" />
        <activity android:name=".Thanks" />
        <activity android:name=".VoteDone" />
        <activity android:name=".VodingChoice" />
        <activity android:name=".Result" />
        <activity android:name=".auth" />
        <activity android:name=".RegisterActivity" />
        <activity android:name=".User_DetailsActivity" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>